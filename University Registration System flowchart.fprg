            <parameter name="EnrolmentYear" type="Integer" array="True"/>
            <parameter name="RegNum" type="Integer" array="True"/>
            <parameter name="Department" type="String" array="True"/>
            <parameter name="InstructorName" type="String" array="True"/>
            <parameter name="InstructorCode" type="String" array="True"/>
        </parameters>
        <body>
            <declare name="NewStu" type="String" array="False" size=""/>
            <declare name="RegNumCheck" type="Integer" array="False" size=""/>
            <output expression="&quot;University details&quot;" newline="True"/>
            <output expression="&quot;Enter your first enrolment term : &quot;" newline="False"/>
            <input variable="EnrolmentTerm[i]"/>
            <output expression="&quot;Enter your enrolment year : &quot;" newline="True"/>
            <input variable="EnrolmentYear[i]"/>
            <while expression="EnrolmentYear[i]&lt;1980 || EnrolmentYear[i]&gt;2022">
                <output expression="&quot;Incorrect year entered, Enter your enrolment year : &quot;" newline="False"/>
                <input variable="EnrolmentYear[i]"/>
            </while>
            <output expression="&quot;Are you a new student ? &quot;" newline="True"/>
            <input variable="NewStu"/>
            <if expression="NewStu==&quot;Yes&quot; || NewStu==&quot;YEs&quot; || NewStu==&quot;YES&quot; || NewStu==&quot;yeS&quot; || NewStu==&quot;yES&quot; || NewStu==&quot;yEs&quot; || NewStu==&quot;yes&quot;">
                <then>
                    <assign variable="RegNum[i]" expression="2022000+i"/>
