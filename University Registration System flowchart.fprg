                </then>
                <else>
                    <output expression="&quot;Enter your registration number : &quot;" newline="True"/>
                    <input variable="RegNum[i]"/>
                    <assign variable="RegNumCheck" expression="RegNum[i]/1000"/>
                    <while expression="RegNumCheck!=EnrolmentYear[i]">
                        <output expression="&quot;Incorrect registration number entered, enter your registration number : &quot;" newline="True"/>
                        <input variable="RegNum[i]"/>
                        <assign variable="RegNumCheck" expression="RegNum[i]/1000"/>
                    </while>
                </else>
            </if>
            <output expression="&quot;Enter your department of enrolment : &quot;" newline="False"/>
            <input variable="Department[i]"/>
            <output expression="&quot;Enter your instructor name : &quot;" newline="True"/>
            <input variable="InstructorName[i]"/>
            <output expression="&quot;Enter your instructor code : &quot;" newline="True"/>
            <input variable="InstructorCode[i]"/>
        </body>
