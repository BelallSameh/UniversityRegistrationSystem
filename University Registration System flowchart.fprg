    <function name="GetPaymentDetails" type="None" variable="">
        <parameters>
            <parameter name="i" type="Integer" array="False"/>
            <parameter name="PaymentMethod" type="String" array="True"/>
            <parameter name="PaymentsCount" type="Integer" array="True"/>
            <parameter name="TotalPayment" type="Integer" array="True"/>
            <parameter name="BankOfTransaction" type="String" array="True"/>
            <parameter name="TransactionNumber" type="String" array="True"/>
            <parameter name="AmountPaid" type="Integer" array="True"/>
            <parameter name="DayOfTransaction" type="Integer" array="True"/>
            <parameter name="MonthOfTransaction" type="Integer" array="True"/>
            <parameter name="YearOfTransaction" type="Integer" array="True"/>
        </parameters>
        <body>
            <declare name="k, j" type="Integer" array="False" size=""/>
            <output expression="&quot;Enter your payment method : &quot;" newline="False"/>
            <input variable="PaymentMethod[i]"/>
            <output expression="&quot;Enter the number of payments done (must be a number) : &quot;" newline="False"/>
            <input variable="PaymentsCount[i]"/>
            <assign variable="k" expression="PaymentsCount[i]"/>
            <output expression="&quot;Enter the total amount payed so far : &quot;" newline="False"/>
            <input variable="TotalPayment[i]"/>
            <for variable="j" start="0" end="k-1" direction="inc" step="1">
                <output expression="&quot;Transaction&quot;" newline="True"/>
                <output expression="&quot;Enter the bank transaction : &quot;" newline="False"/>
                <input variable="BankOfTransaction[j]"/>
                <output expression="&quot;Enter the transaction number : &quot;" newline="False"/>
                <input variable="TransactionNumber[j]"/>
                <output expression="&quot;Enter the amount paid : &quot;" newline="False"/>
                <input variable="AmountPaid[j]"/>
                <output expression="&quot;Enter the day of transaction : &quot;" newline="False"/>
                <input variable="DayOfTransaction[j]"/>
                <while expression="DayOfTransaction[j]&lt;1 || DayOfTransaction[j]&gt;31">
                    <output expression="&quot;Incorrect day entered, Enter the day of transaction : &quot;" newline="False"/>
                    <input variable="DayOfTransaction[j]"/>
                </while>
                <output expression="&quot;Enter the month of transaction : &quot;" newline="False"/>
                <input variable="MonthOfTransaction[j]"/>
                <while expression="MonthOfTransaction[j]&lt;1 || MonthOfTransaction[j]&gt;12">
                    <output expression="&quot;Incorrect month entered, Enter the month of transaction : &quot;" newline="False"/>
                    <input variable="MonthOfTransaction[j]"/>
                </while>
                <output expression="&quot;Enter the year of transaction : &quot;" newline="False"/>
                <input variable="YearOfTransaction[j]"/>
                <while expression="YearOfTransaction[j]&lt;1980 || YearOfTransaction[j]&gt;2022">
                    <output expression="&quot;Incorrect year entered, Enter the year of transaction : &quot;" newline="False"/>
                    <input variable="YearOfTransaction[j]"/>
                </while>
            </for>
        </body>
    </function>
