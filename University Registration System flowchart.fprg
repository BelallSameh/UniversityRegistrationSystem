    <function name="GetContactDetails" type="None" variable="">
        <parameters>
            <parameter name="i" type="Integer" array="False"/>
            <parameter name="Country" type="String" array="True"/>
            <parameter name="Governorate" type="String" array="True"/>
            <parameter name="City" type="String" array="True"/>
            <parameter name="District" type="String" array="True"/>
            <parameter name="StreetName" type="String" array="True"/>
            <parameter name="HouseNumber" type="String" array="True"/>
            <parameter name="FlatNumber" type="String" array="True"/>
            <parameter name="PostalCode" type="String" array="True"/>
            <parameter name="MailingCountry" type="String" array="True"/>
            <parameter name="MailingGovernorate" type="String" array="True"/>
            <parameter name="MailingCity" type="String" array="True"/>
            <parameter name="MailingDistrict" type="String" array="True"/>
            <parameter name="MailingStreetName" type="String" array="True"/>
            <parameter name="MailingHouseNumber" type="String" array="True"/>
            <parameter name="MailingFlatNumber" type="String" array="True"/>
            <parameter name="MailingPostalCode" type="String" array="True"/>
            <parameter name="StudentEmail" type="String" array="True"/>
            <parameter name="PhoneNumber" type="String" array="True"/>
            <parameter name="HomePhoneNumber" type="String" array="True"/>
        </parameters>
        <body>
            <declare name="Mailing" type="String" array="False" size=""/>
            <output expression="&quot;Contact details&quot;" newline="True"/>
            <output expression="&quot;Enter your country : &quot;" newline="False"/>
            <input variable="Country[i]"/>
            <output expression="&quot;Enter your governorate : &quot;" newline="False"/>
            <input variable="Governorate[i]"/>
            <output expression="&quot;Enter your city : &quot;" newline="False"/>
            <input variable="City[i]"/>
            <output expression="&quot;Enter your district : &quot;" newline="False"/>
            <input variable="District[i]"/>
            <output expression="&quot;Enter your street name : &quot;" newline="False"/>
            <input variable="StreetName[i]"/>
            <output expression="&quot;Enter your house number : &quot;" newline="False"/>
            <input variable="HouseNumber[i]"/>
            <output expression="&quot;Enter your flat number : &quot;" newline="False"/>
            <input variable="FlatNumber[i]"/>
            <output expression="&quot;Enter your postal code : &quot;" newline="False"/>
            <input variable="PostalCode[i]"/>
