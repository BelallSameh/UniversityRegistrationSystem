    <function name="RefPassword" type="Integer" variable="z">
        <parameters>
            <parameter name="RegNum" type="Integer" array="True"/>
            <parameter name="Password" type="String" array="True"/>
        </parameters>
        <body>
            <declare name="EntryPassword" type="String" array="False" size=""/>
            <declare name="z, EntryRegNum, i, test" type="Integer" array="False" size=""/>
            <assign variable="test" expression="0"/>
            <output expression="&quot;Enter your reg num : &quot;" newline="True"/>
            <input variable="EntryRegNum"/>
            <output expression="&quot;Enter your password : &quot;" newline="True"/>
            <input variable="EntryPassword"/>
            <for variable="i" start="0" end="1" direction="inc" step="1">
                <if expression="RegNum[i]==EntryRegNum">
                    <then>
                        <assign variable="z" expression="i"/>
                        <assign variable="test" expression="1"/>
                    </then>
                    <else/>
                </if>
            </for>
            <while expression="test==0">
                <output expression="&quot;Incorrect registration number entered, please enter a valid registation number and password&quot;" newline="True"/>
                <output expression="&quot;Enter your reg num : &quot;" newline="True"/>
                <input variable="EntryRegNum"/>
                <output expression="&quot;Enter your password : &quot;" newline="True"/>
                <input variable="EntryPassword"/>
                <for variable="i" start="0" end="1" direction="inc" step="1">
                    <if expression="RegNum[i]==EntryRegNum">
                        <then>
                            <assign variable="z" expression="i"/>
                            <assign variable="test" expression="1"/>
                        </then>
                        <else/>
                    </if>
                </for>
            </while>
            <while expression="Password[z]!=EntryPassword">
                <output expression="&quot;Incorrect password ,Enter your password : &quot;" newline="True"/>
                <input variable="EntryPassword"/>
            </while>
        </body>
    </function>
