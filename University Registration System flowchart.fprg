            <input variable="NewHomePhoneNumber"/>
            <assign variable="TempHomePhoneNumber" expression="HomePhoneNumber[i]"/>
            <assign variable="HomePhoneNumber[i]" expression="NewHomePhoneNumber"/>
            <output expression="&quot;Student contact details before configuration : &quot;" newline="True"/>
            <output expression="&quot;Contact e-mail : &quot; &amp;TempStudentEmail" newline="True"/>
            <output expression="&quot;Phone number : &quot; &amp;TempPhoneNumber" newline="True"/>
            <output expression="&quot;Home phone number : &quot; &amp;TempHomePhoneNumber" newline="True"/>
            <output expression="&quot;Student contact details after configuration : &quot;" newline="True"/>
            <output expression="&quot;Contact e-mail : &quot; &amp;StudentEmail[i]" newline="True"/>
            <output expression="&quot;Phone number : &quot; &amp;PhoneNumber[i]" newline="True"/>
            <output expression="&quot;Home phone number : &quot; &amp;HomePhoneNumber[i]" newline="True"/>
        </body>
    </function>
    <function name="ChangeGpa" type="None" variable="">
        <parameters>
            <parameter name="z" type="Integer" array="False"/>
            <parameter name="FirstName" type="String" array="True"/>
            <parameter name="MiddleName" type="String" array="True"/>
            <parameter name="LastName" type="String" array="True"/>
            <parameter name="RegNum" type="Integer" array="True"/>
            <parameter name="Term" type="Integer" array="True"/>
            <parameter name="Gpa1" type="Real" array="True"/>
            <parameter name="Gpa2" type="Real" array="True"/>
            <parameter name="Gpa3" type="Real" array="True"/>
            <parameter name="Gpa4" type="Real" array="True"/>
            <parameter name="Gpa5" type="Real" array="True"/>
            <parameter name="Gpa6" type="Real" array="True"/>
            <parameter name="Gpa7" type="Real" array="True"/>
            <parameter name="Gpa8" type="Real" array="True"/>
            <parameter name="TotalGpa" type="Real" array="True"/>
        </parameters>
        <body>
            <declare name="Choice, i, Amount, j" type="Integer" array="False" size=""/>
            <declare name="NewGpa, TempGpa1, TempGpa2, TempGpa3, TempGpa4, TempGpa5, TempGpa6, TempGpa7, TempGpa8, TempTotalGpa" type="Real" array="False" size=""/>
            <assign variable="i" expression="z"/>
            <assign variable="TempTotalGpa" expression="TotalGpa[i]"/>
            <output expression="&quot;Student name : &quot;&amp;FirstName[i]" newline="False"/>
            <output expression="&quot; &quot;&amp;MiddleName[i]" newline="False"/>
            <output expression="&quot; &quot;&amp;LastName[i]" newline="True"/>
            <output expression="&quot;Registration number : &quot; &amp;RegNum[i]" newline="True"/>
            <output expression="&quot;How many semesters are you going to change ? (MUST BE A NUMBER FROM 1 TO 8&quot;" newline="True"/>
            <input variable="Amount"/>
            <while expression="Amount&lt;1 || Amount&gt;8">
                <output expression="&quot;Incorrect no. of terms entered , please enter correct number : &quot;" newline="True"/>
                <input variable="Amount"/>
            </while>
            <for variable="j" start="1" end="Amount" direction="inc" step="1">
                <output expression="&quot;Please choose the semester you want to change its GPA (MUST BE A NUMBER FROM 1 TO 8 IN THE CORRECT ORDER OF TERMS)&quot;" newline="True"/>
                <input variable="Choice"/>
                <while expression="Choice&lt;1 || Choice&gt;8">
                    <output expression="&quot;Incorrect term entered , please enter correct term : &quot;" newline="True"/>
                    <input variable="Choice"/>
                </while>
                <output expression="&quot;Enter your new Gpa : &quot;" newline="True"/>
                <input variable="NewGpa"/>
                <while expression="NewGpa&lt;0 || NewGpa&gt;4">
                    <output expression="&quot;Incorrect Gpa entered, Enter your new Gpa : &quot;" newline="True"/>
                    <input variable="NewGpa"/>
                </while>
                <if expression="Choice==1">
                    <then>
                        <assign variable="TempGpa1" expression="Gpa1[i]"/>
                        <assign variable="Gpa1[i]" expression="NewGpa"/>
                        <assign variable="TotalGpa[i]" expression="Gpa1[i]"/>
                        <output expression="&quot;Old Gpa for term 1 : &quot; &amp;TempGpa1" newline="True"/>
                        <output expression="&quot;After configuration : &quot;" newline="True"/>
                        <output expression="&quot;New Gpa for term 1 : &quot; &amp;Gpa1[i]" newline="True"/>
                    </then>
                    <else>
                        <if expression="Choice==2">
                            <then>
                                <assign variable="TempGpa2" expression="Gpa2[i]"/>
                                <assign variable="Gpa2[i]" expression="NewGpa"/>
                                <assign variable="TotalGpa[i]" expression="(Gpa1[i]+Gpa2[i])/2"/>
                                <output expression="&quot;Old Gpa for term 2 : &quot; &amp;TempGpa2" newline="True"/>
                                <output expression="&quot;After configuration : &quot;" newline="True"/>
                                <output expression="&quot;New Gpa for term 2 : &quot; &amp;Gpa2[i]" newline="True"/>
                            </then>
                            <else>
                                <if expression="Choice==3">
                                    <then>
                                        <assign variable="TempGpa3" expression="Gpa3[i]"/>
                                        <assign variable="Gpa3[i]" expression="NewGpa"/>
                                        <assign variable="TotalGpa[i]" expression="(Gpa1[i]+Gpa2[i]+Gpa3[i])/3"/>
                                        <output expression="&quot;Old Gpa for term 3 : &quot; &amp;TempGpa3" newline="True"/>
                                        <output expression="&quot;After configuration : &quot;" newline="True"/>
                                        <output expression="&quot;New Gpa for term 3 : &quot; &amp;Gpa3[i]" newline="True"/>
                                    </then>
                                    <else>
                                        <if expression="Choice==4">
                                            <then>
                                                <assign variable="TempGpa4" expression="Gpa4[i]"/>
                                                <assign variable="Gpa4[i]" expression="NewGpa"/>
                                                <assign variable="TotalGpa[i]" expression="(Gpa1[i]+Gpa2[i]+Gpa3[i]+Gpa4[i])/4"/>
                                                <output expression="&quot;Old Gpa for term 4 : &quot; &amp;TempGpa4" newline="True"/>
                                                <output expression="&quot;After configuration : &quot;" newline="True"/>
                                                <output expression="&quot;New Gpa for term 4 : &quot; &amp;Gpa4[i]" newline="True"/>
                                            </then>
                                            <else>
                                                <if expression="Choice==5">
                                                    <then>
                                                        <assign variable="TempGpa5" expression="Gpa5[i]"/>
                                                        <assign variable="Gpa5[i]" expression="NewGpa"/>
                                                        <assign variable="TotalGpa[i]" expression="(Gpa1[i]+Gpa2[i]+Gpa3[i]+Gpa4[i]+Gpa5[i])/5"/>
                                                        <output expression="&quot;Old Gpa for term 5 : &quot; &amp;TempGpa5" newline="True"/>
                                                        <output expression="&quot;After configuration : &quot;" newline="True"/>
                                                        <output expression="&quot;New Gpa for term 5 : &quot; &amp;Gpa5[i]" newline="True"/>
                                                    </then>
                                                    <else>
                                                        <if expression="Choice==6">
                                                            <then>
                                                                <assign variable="TempGpa6" expression="Gpa6[i]"/>
                                                                <assign variable="Gpa6[i]" expression="NewGpa"/>
                                                                <assign variable="TotalGpa[i]" expression="(Gpa1[i]+Gpa2[i]+Gpa3[i]+Gpa4[i]+Gpa5[i]+Gpa6[i])/6"/>
                                                                <output expression="&quot;Old Gpa for term 6 : &quot; &amp;TempGpa6" newline="True"/>
                                                                <output expression="&quot;After configuration : &quot;" newline="True"/>
                                                                <output expression="&quot;New Gpa for term 6 : &quot; &amp;Gpa6[i]" newline="True"/>
                                                            </then>
                                                            <else>
                                                                <if expression="Choice==7">
                                                                    <then>
                                                                        <assign variable="TempGpa7" expression="Gpa7[i]"/>
                                                                        <assign variable="Gpa7[i]" expression="NewGpa"/>
                                                                        <assign variable="TotalGpa[i]" expression="(Gpa1[i]+Gpa2[i]+Gpa3[i]+Gpa4[i]+Gpa5[i]+Gpa6[i]+Gpa7[i])/7"/>
                                                                        <output expression="&quot;Old Gpa for term 7 : &quot; &amp;TempGpa7" newline="True"/>
                                                                        <output expression="&quot;After configuration : &quot;" newline="True"/>
                                                                        <output expression="&quot;New Gpa for term 7 : &quot; &amp;Gpa7[i]" newline="True"/>
                                                                    </then>
                                                                    <else>
                                                                        <assign variable="TempGpa8" expression="Gpa8[i]"/>
                                                                        <assign variable="Gpa8[i]" expression="NewGpa"/>
                                                                        <assign variable="TotalGpa[i]" expression="(Gpa1[i]+Gpa2[i]+Gpa3[i]+Gpa4[i]+Gpa5[i]+Gpa6[i]+Gpa7[i]+Gpa8[i])/8"/>
                                                                        <output expression="&quot;Old Gpa for term 8 : &quot; &amp;TempGpa8" newline="True"/>
                                                                        <output expression="&quot;After configuration : &quot;" newline="True"/>
                                                                        <output expression="&quot;New Gpa for term 8 : &quot; &amp;Gpa8[i]" newline="True"/>
                                                                    </else>
                                                                </if>
                                                            </else>
                                                        </if>
                                                    </else>
                                                </if>
                                            </else>
                                        </if>
                                    </else>
                                </if>
                            </else>
                        </if>
                    </else>
                </if>
            </for>
            <output expression="&quot;Total Gpa before configuration : &quot; &amp;TempTotalGpa" newline="True"/>
            <output expression="&quot;Total Gpa after configuration : &quot; &amp;TotalGpa[i]" newline="True"/>
        </body>
    </function>
