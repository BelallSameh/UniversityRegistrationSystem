    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="FirstName, MiddleName, LastName, Gender, Nationality, IdType, StudentId, Password, Country, Governorate, City, District, StreetName, HouseNumber, FlatNumber, PostalCode, MailingCountry, MailingGovernorate, MailingCity, MailingDistrict, MailingStreetName, MailingHouseNumber, MailingFlatNumber, MailingPostalCode, StudentEmail, PhoneNumber, HomePhoneNumber, EnrolmentTerm, Department, InstructorName, InstructorCode, PaymentMethod, BankOfTransaction, TransactionNumber" type="String" array="True" size="5"/>
            <declare name="DayOfBirth, MonthOfBirth, YearOfBirth, EnrolmentYear, RegNum, FinishedCreditHours, Term, PaymentsCount, TotalPayment, AmountPaid, DayOfTransaction, MonthOfTransaction, YearOfTransaction, MaxCreditHours, CurrentCreditHours, RemainingCreditHours" type="Integer" array="True" size="5"/>
            <declare name="TotalGpa, Gpa1, Gpa2, Gpa3, Gpa4, Gpa5, Gpa6, Gpa7, Gpa8" type="Real" array="True" size="5"/>
            <call expression="ScanStudentData(FirstName,MiddleName,LastName,Gender,Nationality,DayOfBirth,MonthOfBirth,YearOfBirth,IdType,StudentId,Password,Country,Governorate,City,District,StreetName,HouseNumber,FlatNumber,PostalCode,MailingCountry, MailingGovernorate, MailingCity, MailingDistrict, MailingStreetName, MailingHouseNumber, MailingFlatNumber, MailingPostalCode,StudentEmail,PhoneNumber,HomePhoneNumber,EnrolmentTerm,EnrolmentYear,RegNum,Department, InstructorName, InstructorCode,FinishedCreditHours,Term,PaymentMethod,PaymentsCount,TotalPayment,BankOfTransaction,TransactionNumber,AmountPaid,DayOfTransaction,MonthOfTransaction,YearOfTransaction,MaxCreditHours, CurrentCreditHours,RemainingCreditHours,TotalGpa,Gpa1,Gpa2,Gpa3,Gpa4,Gpa5,Gpa6,Gpa7,Gpa8)"/>
            <declare name="z, choice, ProgramTerminator, StudentTerminator" type="Integer" array="False" size=""/>
            <declare name="Continue" type="String" array="False" size=""/>
            <assign variable="ProgramTerminator" expression="1"/>
            <while expression="ProgramTerminator==1">
                <assign variable="z" expression="RefPassword(RegNum,Password)"/>
                <assign variable="StudentTerminator" expression="1"/>
                <while expression="StudentTerminator==1">
                    <output expression="&quot;List of function&#13;&#10;1 - Print Student Data&#13;&#10;2- Calculate student age&#13;&#10;3- Calculate the number of credit hours&#13;&#10;4- Configure Semester Gpa&#13;&#10;5- Configure contact details&#13;&#10;Choose the funcion required&quot;" newline="True"/>
                    <input variable="Choice"/>
                    <while expression="Choice&lt;1 || Choice&gt;5">
                        <output expression="&quot;Incorrect choice entered, Please enter correct choice from the list below : &quot;" newline="True"/>
                        <output expression="&quot;List of function&#13;&#10;1 - Print Student Data&#13;&#10;2- Calculate student age&#13;&#10;3- Calculate the number of credit hours&#13;&#10;4- Configure Semester Gpa&#13;&#10;5- Configure contact details&#13;&#10;Choose the funcion required&quot;" newline="True"/>
                        <input variable="Choice"/>
                    </while>
                    <if expression="Choice==1">
                        <then>
                            <call expression="PrintStudentData(z,FirstName,MiddleName,LastName,Gender,Nationality,DayOfBirth,MonthOfBirth,YearOfBirth,IdType,StudentId,Password,Country,Governorate,City,District,StreetName,HouseNumber,FlatNumber,PostalCode,MailingCountry, MailingGovernorate, MailingCity, MailingDistrict, MailingStreetName, MailingHouseNumber, MailingFlatNumber, MailingPostalCode,StudentEmail,PhoneNumber,HomePhoneNumber,EnrolmentTerm,EnrolmentYear,RegNum,Department, InstructorName, InstructorCode,FinishedCreditHours,Term,PaymentMethod,PaymentsCount,TotalPayment,BankOfTransaction,TransactionNumber,AmountPaid,DayOfTransaction,MonthOfTransaction,YearOfTransaction,MaxCreditHours, CurrentCreditHours,RemainingCreditHours,TotalGpa,Gpa1,Gpa2,Gpa3,Gpa4,Gpa5,Gpa6,Gpa7,Gpa8)"/>
                        </then>
                        <else>
                            <if expression="Choice==2">
                                <then>
                                    <call expression="CalcAge(z,FirstName,MiddleName,LastName,RegNum,DayOfBirth,MonthOfBirth,YearOfBirth)"/>
                                </then>
                                <else>
                                    <if expression="Choice==3">
                                        <then>
                                            <call expression="CreditHours(z,FirstName,MiddleName,LastName,RegNum,Term,FinishedCreditHours,MaxCreditHours,CurrentCreditHours,RemainingCreditHours)"/>
                                        </then>
                                        <else>
                                            <if expression="Choice==4 &amp;&amp; Term[z]!=1">
                                                <then>
                                                    <call expression="ChangeGpa(z,FirstName,MiddleName,LastName,RegNum,Term,Gpa1,Gpa2,Gpa3,Gpa4,Gpa5,Gpa6,Gpa7,Gpa8,TotalGpa)"/>
                                                </then>
                                                <else>
                                                    <if expression="Choice==5">
                                                        <then>
                                                            <call expression="ChangeContactDetails(z,FirstName,MiddleName,LastName,RegNum,StudentEmail, PhoneNumber,HomePhoneNumber)"/>
                                                        </then>
                                                        <else/>
                                                    </if>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                    <if expression="Choice==4 &amp;&amp; Term[z]==1">
                        <then>
                            <output expression="&quot;Student is still at term 1 . GPA CANNOT BE CHANGED&quot;" newline="True"/>
                        </then>
                        <else/>
                    </if>
                    <output expression="&quot;Do you want to perform another function for student &quot; &amp;RegNum[z]" newline="True"/>
                    <input variable="Continue"/>
                    <if expression="Continue==&quot;Yes&quot; || Continue==&quot;YEs&quot; || Continue==&quot;YES&quot; || Continue==&quot;yeS&quot; || Continue==&quot;yES&quot; || Continue==&quot;yEs&quot; || Continue==&quot;yes&quot;">
                        <then>
                            <assign variable="StudentTerminator" expression="1"/>
                        </then>
                        <else>
                            <assign variable="StudentTerminator" expression="0"/>
                            <output expression="&quot;Thanks for using AASTMT registration system&quot;" newline="True"/>
                        </else>
                    </if>
                </while>
                <output expression="&quot;Do you want to perform another function for another student &quot;" newline="True"/>
                <input variable="Continue"/>
                <if expression="Continue==&quot;Yes&quot; || Continue==&quot;YEs&quot; || Continue==&quot;YES&quot; || Continue==&quot;yeS&quot; || Continue==&quot;yES&quot; || Continue==&quot;yEs&quot; || Continue==&quot;yes&quot;">
                    <then>
                        <assign variable="ProgramTerminator" expression="1"/>
                    </then>
                    <else>
                        <assign variable="ProgramTerminator" expression="0"/>
                    </else>
                </if>
            </while>
            <output expression="&quot;Thanks for using AASTMT registration system&quot;" newline="True"/>
        </body>
    </function>
    <function name="CalcAge" type="None" variable="">
        <parameters>
            <parameter name="z" type="Integer" array="False"/>
            <parameter name="FirstName" type="String" array="True"/>
            <parameter name="MiddleName" type="String" array="True"/>
            <parameter name="LastName" type="String" array="True"/>
            <parameter name="RegNum" type="Integer" array="True"/>
            <parameter name="DayOfBirth" type="Integer" array="True"/>
            <parameter name="MonthOfBirth" type="Integer" array="True"/>
            <parameter name="YearOfBirth" type="Integer" array="True"/>
        </parameters>
        <body>
            <declare name="Day, Month, year, Age, i" type="Integer" array="False" size=""/>
            <assign variable="i" expression="z"/>
            <output expression="&quot;Student name : &quot;&amp;FirstName[i]" newline="False"/>
            <output expression="&quot; &quot;&amp;MiddleName[i]" newline="False"/>
            <output expression="&quot; &quot;&amp;LastName[i]" newline="True"/>
            <output expression="&quot;Registration number : &quot; &amp;RegNum[i]" newline="True"/>
            <output expression="&quot;Enter today date by day, month and year : &quot;" newline="True"/>
            <output expression="&quot;Day : &quot;" newline="True"/>
            <input variable="Day"/>
            <while expression="Day&lt;1 || Day&gt;31">
                <output expression="&quot;Incorrect day entered, Enter correct day : &quot;" newline="False"/>
                <input variable="Day"/>
            </while>
            <output expression="&quot;Month : &quot;" newline="True"/>
            <input variable="Month"/>
            <while expression="Month&lt;1 || Month&gt;12">
                <output expression="&quot;Incorrect month entered, Enter correct month : &quot;" newline="False"/>
                <input variable="Month"/>
            </while>
            <output expression="&quot;Year : &quot;" newline="True"/>
            <input variable="Year"/>
            <while expression="Year&lt;1980 || Year&gt;2022">
                <output expression="&quot;Incorrect year entered, Enter correct year : &quot;" newline="False"/>
                <input variable="Year"/>
            </while>
            <assign variable="Age" expression="(Year-YearOfBirth[i])+((Month-MonthOfBirth[i])/12)+((Day-DayofBirth[i])/365)"/>
            <output expression="&quot;Student Age : &quot; &amp;Age" newline="True"/>
            <output expression="&quot;at &quot; &amp;Day" newline="False"/>
            <output expression="&quot; / &quot; &amp;Month" newline="False"/>
            <output expression="&quot; / &quot; &amp;Year" newline="True"/>
        </body>
    </function>
    <function name="ChangeContactDetails" type="None" variable="">
        <parameters>
            <parameter name="z" type="Integer" array="False"/>
            <parameter name="FirstName" type="String" array="True"/>
            <parameter name="MiddleName" type="String" array="True"/>
            <parameter name="LastName" type="String" array="True"/>
            <parameter name="RegNum" type="Integer" array="True"/>
            <parameter name="StudentEmail" type="String" array="True"/>
            <parameter name="PhoneNumber" type="String" array="True"/>
            <parameter name="HomePhoneNumber" type="String" array="True"/>
        </parameters>
        <body>
            <declare name="i, Choice" type="Integer" array="False" size=""/>
            <declare name="NewStudentEmail, NewPhoneNumber, NewHomePhoneNumber, TempStudentEmail, TempPhoneNumber, TempHomePhoneNumber" type="String" array="False" size=""/>
            <assign variable="i" expression="z"/>
            <output expression="&quot;Student name : &quot;&amp;FirstName[i]" newline="False"/>
            <output expression="&quot; &quot;&amp;MiddleName[i]" newline="False"/>
            <output expression="&quot; &quot;&amp;LastName[i]" newline="True"/>
            <output expression="&quot;Registration number : &quot; &amp;RegNum[i]" newline="True"/>
            <output expression="&quot;Please enter the new student mail : &#13;&#10;(if you do not want to change your mail please enter the same mail&quot;" newline="True"/>
            <input variable="NewStudentEmail"/>
            <assign variable="TempStudentEmail" expression="StudentEmail[i]"/>
            <assign variable="StudentEmail[i]" expression="NewStudentEmail"/>
            <output expression="&quot;Please enter the new phone number : &#13;&#10;(if you do not want to change your phone number please enter the same number&quot;" newline="True"/>
            <input variable="NewPhoneNumber"/>
            <assign variable="TempPhoneNumber" expression="PhoneNumber[i]"/>
            <assign variable="PhoneNumber[i]" expression="NewPhoneNumber"/>
