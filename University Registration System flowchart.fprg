    <function name="GetCreditHours" type="None" variable="">
        <parameters>
            <parameter name="i" type="Integer" array="False"/>
            <parameter name="FinishedCreditHours" type="Integer" array="True"/>
            <parameter name="Term" type="Integer" array="True"/>
            <parameter name="MaxCreditHours" type="Integer" array="True"/>
            <parameter name="CurrentCreditHours" type="Integer" array="True"/>
            <parameter name="RemainingCreditHours" type="Integer" array="True"/>
        </parameters>
        <body>
            <output expression="&quot;Enter your finished credit hours : &quot;" newline="False"/>
            <input variable="FinishedCreditHours[i]"/>
            <while expression="FinishedCreditHours[i]&lt;0 || FinishedCreditHours[i]&gt;144">
                <output expression="&quot;Incorrect credit hours entered, Enter your finished credit hours : &quot;" newline="False"/>
                <input variable="FinishedCreditHours[i]"/>
            </while>
            <assign variable="Term[i]" expression="(FinishedCreditHours[i]/18)+1"/>
            <output expression="&quot;Enter the maximum credit hours required for the degree : &quot;" newline="False"/>
            <input variable="MaxCreditHours[i]"/>
            <output expression="&quot;Enter the current credit hours : &quot;" newline="False"/>
            <input variable="CurrentCreditHours[i]"/>
            <assign variable="RemainingCreditHours[i]" expression="MaxCreditHours[i]-(FinishedCreditHours[i]+CurrentCreditHours[i])"/>
        </body>
    </function>
